{
    "name": "Greenway Extension",
    "version": "1.0",
    "description": "Checking tech inspect date of cars from gelios api",
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "storage",
        "activeTab",
        "scripting",
        "nativeMessaging",
        "unlimitedStorage",
        "webRequest",
        "webNavigation"
    ],
    "host_permissions": [
        "https://*.greenway.ge/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "assets/MygpsLogo.png",
            "32": "assets/MygpsLogo.png",
            "48": "assets/MygpsLogo.png",
            "128": "assets/MygpsLogo.png"
        }
    },
    "icons": {
        "16": "assets/MygpsLogo.png",
        "32": "assets/MygpsLogo.png",
        "48": "assets/MygpsLogo.png",
        "128": "assets/MygpsLogo.png"
    }
}